{
  "name": "@yomguithereal/sigma-experiments-renderers",
  "version": "0.2.1",
  "description": "Various alternative renderers for sigma.js v2.",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "edge",
    "node",
    "index.js",
    "*.d.ts"
  ],
  "scripts": {
    "dist": "tsc -p ./tsconfig.transpile.json -d",
    "clean": "rm -rf edge node *.js *.d.ts",
    "dev": "kotatsu serve ftest/ftest.ts --index ftest/index.html --open",
    "prepare": "npm run dist",
    "prettier": "prettier --write '**/*.ts'",
    "postpublish": "npm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yomguithereal/sigma-experiments.git"
  },
  "keywords": [
    "graph",
    "graphology",
    "sigma"
  ],
  "contributors": [
    {
      "name": "Guillaume Plique",
      "url": "http://github.com/Yomguithereal"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yomguithereal/sigma-experiments/issues"
  },
  "homepage": "https://github.com/Yomguithereal/sigma-experiments/renderers#readme",
  "devDependencies": {
    "graphology": "^0.25.1",
    "graphology-components": "^1.5.4",
    "graphology-generators": "^0.11.2",
    "graphology-layout": "^0.6.1",
    "graphology-layout-forceatlas2": "^0.10.1",
    "graphology-types": "0.24.5",
    "kotatsu": "^0.23.1",
    "prettier": "^2.7.1",
    "sigma": "3.0.0-alpha2",
    "typescript": "^4.8.4"
  }
}
